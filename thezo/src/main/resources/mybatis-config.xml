<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.thezo.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.thezo.chatting.model.vo.Chatting" alias="Chatting"/>
		<typeAlias type="com.kh.thezo.message.model.vo.Message" alias="Message"/>
		<typeAlias type="com.kh.thezo.notification.model.vo.Notification" alias="Notification"/>
		<typeAlias type="com.kh.thezo.statistic.model.vo.Statistic" alias="Statistic"/>
		<typeAlias type="com.kh.thezo.document.model.vo.Document" alias="Document"/>
		
	</typeAliases>
	
	<mappers>
		<mapper resource="/mappers/member-mapper.xml"/>
		<mapper resource="/mappers/chatting-mapper.xml"/>
		<mapper resource="/mappers/common-mapper.xml"/>
		<mapper resource="/mappers/message-mapper.xml"/>
		<mapper resource="/mappers/notification-mapper.xml"/>
		<mapper resource="/mappers/statistic-mapper.xml"/>
		<mapper resource="/mappers/document-mapper.xml"/>			
	</mappers>

</configuration>
